   "source": [
    "def TowerOfHanoi(n,from_rod, to_rod,aux_rod):\n",
    "    if n==0:\n",
    "        return \n",
    "    TowerOfHanoi(n-1,from_rod,aux_rod,to_rod)\n",
    "    print(\"Move disk\",n,'form rod',from_rod,\"to rod\",to_rod)\n",
    "    TowerOfHanoi(n-1,aux_rod,to_rod,from_rod)\n",
    "    \n",
    "n = 3\n",
    "TowerOfHanoi(n,'A','C','B')"
   ]
